msgstr ""

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:437(term)
msgid "--configdir &lt;config directory&gt;"
msgstr ""

#: /home/ger/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:439(para)
