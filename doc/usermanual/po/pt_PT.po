msgstr "Horas da informação EXIF da imagem (24h)"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6293(para)
msgid "Hours from the image EXIF data (12h, AM/PM)"
msgstr "Horas da informação EXIF da imagem (12h, AM/PM)"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6294(para)
msgid "Day from the image EXIF data"
msgstr "Dia da informação EXIF da imagem"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6298(para)
msgid "Abbreviated year from the image EXIF data (2013 is \"13\")"
msgstr "Ano abreviado da informação EXIF da imagem (2013 é \"13\")"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6299(para)
msgid "Full year from the image EXIF data"
msgstr "Ano da informação EXIF da imagem"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6300(para)
msgid "Current system date"
msgstr "Hora actual do sistema"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6301(para)
msgid "Current system time seconds"
msgstr "Hora actual do sistema - segundos"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6302(para)
msgid "Current system time minutes"
msgstr "Hora actual do sistema - minutos"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6303(para)
msgid "Current system time hours (24h)"
msgstr "Hora actual do sistema - horas (24h)"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6304(para)
msgid "Current system time hours (12, AP/PM)"
msgstr "Hora actual do sistema - horas (12, AM/PM)"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6305(para)
msgid "Current system time day"
msgstr "Hora actual do sistema - dia"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6306(para)
msgid "Current system time month"
msgstr "Hora actual do sistema - mês"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6309(para)
msgid "Current system time year (abbreviated)"
msgstr "Hora actual do sistema - ano (abreviado)"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6310(para)
msgid "Current system time year"
msgstr "Hora actual do sistema - ano"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6311(para)
msgid "The maker of camera model"
msgstr "O fabricante da câmara"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6312(para)
msgid "The camera model"
msgstr "O modelo da câmara"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6313(para)
msgid "The specific lens used"
msgstr "A lente utilizada"

#: /home/jcrc/darktable/build/doc/usermanual/po/darktable-usermanual_profiled.xml:6314(para)
msgid "The creator string"
