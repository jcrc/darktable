#: /usr/local/src/darktable/darktable-1.4.0/doc/usermanual/po/darktable-usermanual_profiled.xml:19415(programlisting)
msgid ""
"dt = require \"darktable\" local buffer_count = 5 for i=1,buffer_count do "
"local saved_selection dt.register_event(\"shortcut\",function() "
"saved_selection = dt.gui.selection() end,\"save to buffer \"..i) dt."
"register_event(\"shortcut\",function() dt.gui.selection(saved_selection) end,"
"\"restore from buffer \"..i) end local bounce_buffer = {} dt."
"register_event(\"shortcut\",function() bounce_buffer = dt.gui."
"selection(bounce_buffer) end,\"switch selection with temporary buffer\")"
msgstr ""
"dt = require \"darktable\" local buffer_count = 5 for i=1,buffer_count do "
"local saved_selection dt.register_event(\"shortcut\",function() "
"saved_selection = dt.gui.selection() end,\"save to buffer \"..i) dt."
"register_event(\"shortcut\",function() dt.gui.selection(saved_selection) end,"
"\"restore from buffer \"..i) end local bounce_buffer = {} dt."
"register_event(\"shortcut\",function() bounce_buffer = dt.gui."
"selection(bounce_buffer) end,\"switch selection with temporary buffer\")"

